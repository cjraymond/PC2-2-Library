
def main():
    print "welcome to the library."
    print "What would you like to do today?"
    print "1. Check out an item. /n2. Return a book. /n3. Pay a fee."
    userdo = 0
    while userdo != 1 or != 2 or != 3:
        userdo = int (input ('Please input one of the number choices from above. (Just a number ex. "1", "2", "3")'))
    if userdo = 1:
        checkout ()
    elsif userdo = 2:
        bookreturn ()
    elsif userdo = 3:
        payfees ()

